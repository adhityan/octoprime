{
  "name": "Octoprime",
  "version": "$VERSION",
  "manifest_version": 2,
  "author": "Adhityan",
  "description": "Super addon for GitHub",
  "homepage_url": "https://github.com/adhityan/octotree",
  "icons": {
    "16": "icons/icon128.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon128.png",
      "19": "icons/icon19.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "https://github.com/*",
    "storage"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
